{
    "name": "larashed/agent",
    "description": "Larashed - Laravel monitoring toolkit",
    "type": "library",
    "homepage": "https://larashed.com",
    "keywords": [
        "laravel",
        "monitoring",
        "agent",
        "toolkit"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ignas Bernotas",
            "email": "ignas@iber.lt"
        },
        {
            "name": "Deividas Dziovalas",
            "email": "d.dziovalas@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.0",
        "orchestra/testbench": "5.*",
        "mockery/mockery": "^1.0",
        "dms/phpunit-arraysubset-asserts": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Larashed\\Agent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larashed\\Agent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larashed\\Agent\\AgentServiceProvider"
            ]
        }
    }
}
